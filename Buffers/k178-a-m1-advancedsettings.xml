<advancedsettings>
    <video>
        <allowmpeg4vaapi>true</allowmpeg4vaapi>
        <checkdxvacompatibility>true</checkdxvacompatibility>
        <fpsdetect>1</fpsdetect>
<fullscreenonmoviestart>true</fullscreenonmoviestart>
<enablehighqualityhwscalers>true</enablehighqualityhwscalers>
        <dxvaallowhqscaling>false</dxvaallowhqscaling>
    </video>
<gui>
 <algorithmdirtyregions>3</algorithmdirtyregions>
 <nofliptimeout>0</nofliptimeout>
      </gui>
    <network>
 <alwaysforcebuffer>true</alwaysforcebuffer>
 <autodetectpingtime>30</autodetectpingtime>
        <curlclienttimeout>60</curlclienttimeout>
        <curllowspeedtime>60</curllowspeedtime>
 <disableipv6>true</disableipv6>
 <curlretries>2</curlretries>
    </network>
 <cache>
        <buffermode>0</buffermode>
        <curlclienttimeout>60</curlclienttimeout>
        <curllowspeedtime>60</curllowspeedtime>
		<freememorycachepercent>15</freememorycachepercent>
        <memorysize>145715200</memorysize>
        <readfactor>5.00</readfactor>
    </cache>
    <airplayport>20</airplayport>
    <airtunesport>2</airtunesport>
    <enableairtunesdebuglog>true</enableairtunesdebuglog>
</advancedsettings>